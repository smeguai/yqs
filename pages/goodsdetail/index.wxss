.container {
  padding-bottom: 98rpx;
}
.container .cut-wrapper {
  display: flex;
  background: #FFDE4E;
  height: 68rpx;
  align-items: center;
  padding: 0 24rpx;
}
.container .cut-wrapper .icon {
  width: 32rpx;
  height: 32rpx;
}
.container .cut-wrapper .txt {
  padding: 0 12rpx;
  font-size: 28rpx;font-weight: 500;
}
.container .section .cuttips {
  font-size: 24rpx;
  color:#666;
  padding: 34rpx 0;
  text-align: center;
}
.container .section .cutbtn {
  height: 74rpx;
  line-height: 74rpx;
  background: #FFDE4E;
  font-size: 32rpx;
  font-weight: 500;
  text-align: center;
  border-radius: 38rpx;
}
.container .section .cutheader {
  font-size: 32rpx;
  font-weight: 500;
  padding: 30rpx 0;
}
.container .section .cutlist .cutitem {
  display: flex;
  align-items: center;
  padding: 10rpx 0;
}
.container .section .cutlist .cutitem .cutimg {
  width: 42rpx;
  height: 42rpx;
  border-radius: 100%;
}
.container .section .cutlist .cutitem .content {
  font-size: 24rpx;
  flex: 1;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.container .section .cutlist .cutitem .date {
  font-size: 24rpx;
}
.container .limit-wrapper {
  height: 120rpx;
  display: flex;
}

.container .limit-wrapper .lf {
  background: #ffde4e;
  flex: 1;
  padding: 16rpx 24rpx;
  box-sizing: border-box;
}

.container .limit-wrapper .lf .top {
  display: flex;
  align-items: center;
}

.container .limit-wrapper .lf .top .price {
  font-size: 46rpx;
  font-weight: bold;
  color: #f60;
  padding-right: 20rpx;
}

.container .limit-wrapper .lf .top .txt {
  padding: 2rpx 12rpx;
  background: #f60;
  border-radius: 4rpx;
  color: #fff;
  font-size: 24rpx;
  position: relative;
}

.container .limit-wrapper .lf .top .txt::after {
  content: '';
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: -26rpx;
  border-width: 16rpx;
  border-style: solid;
  border-color: transparent #f60 transparent transparent;
}

.container .limit-wrapper .lf .top .tagprice {
  font-size: 28rpx;
  padding-left: 24rpx;
}

.container .limit-wrapper .lf .count {
  font-size: 24rpx;
}

.container .limit-wrapper .rt {
  background: #333;
  padding: 0 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  position: relative;
}
.container .limit-wrapper .rt::after {
  position: absolute;
  content: '';
  top: 50%;
  transform: translateY(-50%);
  left: -40rpx;
  display: block;
  border-width: 20rpx;
  border-style: solid;
  border-color: transparent #333 transparent transparent;
}
.container .limit-wrapper .rt .top {
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .limit-wrapper .rt .top .icon {
  width: 28rpx;
  height: 28rpx;
}
.container .limit-wrapper .rt .top .txt {
  color: #fff;
  font-size: 28rpx;
  padding-left: 10rpx;
}
.container .banner-wrapper {
  position: relative;
}

.container .banner-wrapper .swiper {
  height: 480rpx;
}

.container .banner-wrapper .swiper .item {
  height: 480rpx;
}

.container .banner-wrapper .swiper .item .img {
  height: 100%;
  width: 100%;
}

.container .banner-wrapper .txt {
  position: absolute;
  right: 0;
  border-radius: 25rpx 0 0 25rpx;
  background: rgba(0, 0, 0, 0.4);
  font-size: 20rpx;
  color: #fff;
  padding: 10rpx 14rpx 10rpx 32rpx;
}
.container .banner-wrapper .txt0 {
  bottom: 20rpx;
}
.container .banner-wrapper .txt1 {
  bottom: 140rpx;
}
.container .banner-wrapper .txt2 {
  bottom: 80rpx;
}
.container .banner-wrapper .anit {
  position: absolute;
  top: 0;
  left: 20rpx;
  border-radius: 24rpx;
  background: #ffde4e;
  font-size: 24rpx;
  color: #333;
  padding: 4rpx 12rpx 4rpx 4rpx;
  opacity: 0;
  display: flex;
  align-items: center;
  animation: paysanition 4s ease infinite;
}

@keyframes paysanition {
  0% {
    top: 200rpx;
    opacity: 0;
  }

  60% {
    top: 260rpx;
    opacity: 1;
  }

  100% {
    top: 240rpx;
    opacity: 0;
  }
}

.container .banner-wrapper .anit .avatar {
  width: 44rpx;
  height: 44rpx;
  border-radius: 100%;
}

.container .banner-wrapper .anit .name {
  padding-left: 14rpx;
}

.container .banner-wrapper .genre {
  position: absolute;
  left: 46rpx;
  top: 0;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}

.container .banner-wrapper .genre .icon {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background: red;
}

.container .banner-wrapper .genre .txt {
  font-size: 36rpx;
  z-index: 2;
  position: relative;
  color: #fff;
}

.container .banner-wrapper .nums {
  position: absolute;
  bottom: 20rpx;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .banner-wrapper .nums .item {
  background: #fff;
  width: 10rpx;
  height: 10rpx;
  border-radius: 10rpx;
  margin: 0 4rpx;
}

.container .banner-wrapper .nums .item.active {
  width: 20rpx;
  background: #ffde4e;
}

.container .section {
  padding: 0 24rpx 48rpx 24rpx;
  background: #fff;
}

.container .section .name {
  font-size: 32rpx;
  font-weight: 500;
  padding-top: 32rpx;
}

.container .section .subtxt {
  font-size: 24rpx;
  padding-top: 20rpx;
}

.container .section .bott {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.container .section .bott .price {
  font-size: 52rpx;
  color: #f60;
  font-weight: bold;
  padding-top: 16rpx;
}

.container .section .bott .price .group {
  font-size: 20rpx;
  color: #f60;
