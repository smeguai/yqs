.list-wrapper {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 18rpx 30rpx;
  box-sizing: border-box;
  background: #fff;
}
.list-wrapper .item {
  width: 330rpx;
  padding: 30rpx 0;
}
.list-wrapper .item .img {
  width: 100%;
  height: 220rpx;
  border-radius: 6rpx;
}
.list-wrapper .item .midd {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 30rpx 0;
}
.list-wrapper .item .midd .price {
  font-size: 36rpx;
  color: #FF6600;
  font-weight: bold;
}
.list-wrapper .item .midd .price .tagprice {
  font-size: 24rpx;
  padding-left: 6rpx;
  font-weight: 400;
  color: #999999;
}
.list-wrapper .item .midd .paynum {
  font-size: 24rpx;
  color: #999999;
  font-weight: 400;
}
.list-wrapper .item .name {
  font-size: 28rpx;
}
.nav {
  display: flex;
  background: #fff;
  padding: 0 24rpx;
  justify-content: space-between;
  border-bottom: 1rpx solid #F6F6F6;
}

.nav .item {
  text-align: center;
}
.nav .item {
  border-radius: 2rpx;
}
.nav .item.active text {
  border-bottom: 3rpx solid #FDDD52;
}
.nav .item text {
  display: inline-block;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.nav .item image {
  width: 22rpx;
  height: 12rpx;
  padding-left: 12rpx;
}