.container .aside {
  background: rgba(0, 0, 0, .4);
  border-radius: 40rpx;
  position: fixed;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  padding: 10rpx 0;
  z-index: 99;
}

.container .aside .item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 0 10rpx;
  background: transparent;
}
.container .aside .item::after  {
  border: none;
}
.container .aside .item .icon {
  width: 34rpx;
  height: 34rpx;
}
.container .aside .item .txt {
  font-size: 20rpx;
  color: #fff;
}
.container .section {
  background: #fff;
  position: relative;
  margin-bottom: 20rpx;
}

.container .section .topbackground{
  height:364rpx;
  background: #FFDE4E;
  border-radius: 0 0 100% 100%;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
}
.container .main {
  padding-top: 120rpx;
}
.container .section .info {
  background: #fff;
  margin: 0 24rpx 0 24rpx;
  position: relative;
  z-index: 10;
  border-radius: 10rpx;
  text-align: center;
  padding: 82rpx 24rpx 64rpx 24rpx;
}
.container .section .info .avatar {
  width: 146rpx;
  height: 146rpx;
  border-radius: 100%;
  border: 2rpx solid #FFDE4E;
  position: absolute;
  top: -74rpx;
  left: 50%;
  transform: translateX(-50%);
}
.container .section .info .name {
  font-size: 24rpx;
  color: #666666;
}
.container .section .info .sub {
  font-size: 32rpx;
  padding: 40rpx 0;
}
.container .section .info .goods {
  padding: 16rpx 0 16rpx 18rpx;
  display: flex;
  background: #F9F9F9;
}
.container .section .info .goods .img {
  width: 156rpx;
  height: 156rpx;
  border-radius: 6rpx;
}
.container .section .info .goods .desc {
  text-align: left;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-left: 26rpx;
  width: 0;
  flex: 1;
}
.container .section .info .goods .desc .title {
  font-size: 28rpx;
}
.container .section .info .goods .desc .mid text {
  font-size: 24rpx;
}
.container .section .info .goods .desc .bott {
  display: flex;
  justify-content: space-between;
}
.container .section .info .goods .desc .bott .price {
  font-size: 28rpx;
  color:#FF6600;
  font-weight: 500;
}
.container .section .info .goods .desc .bott .price .tagprice {
  font-weight: 400;
  color:#999999;
  font-size: 24rpx;
  padding-left: 18rpx;
}
.container .section .info .goods .desc .bott .tab {
  background: #FFDE4E;
  font-size: 20rpx;
  padding:8rpx 20rpx;
  border-radius: 16rpx 0 0 16rpx;
}
.container .section .info .cut {
  padding: 68rpx 0 32rpx 0;
  font-size: 28rpx;
}
.container .section .info .cut .price {
  color: #FF6600;
}
.container .section .info .progdesc {
  display: flex;
  justify-content: space-between;
  font-size: 20rpx;
  color: #999999;
  padding: 24rpx 0 74rpx 0;
}
.container .section .info .btn {
  height: 74rpx;
  line-height: 74rpx;
  border-radius: 38rpx;
  font-size: 32rpx;
}
.container .section .info .btn.btn0 {
  margin-bottom: 48rpx;
  background: #FFDE4E;
}
.container .section .info .btn.btn0::after {
  border: none;
}
.container .section .info .btn.btn1 {
  border: 1rpx solid #FFDE4E;
  box-sizing: border-box;
}
.container .section .info .btn.btn1 .price {
  color: #FF6600;
}
.container .block {
  padding: 0 24rpx;
}
.container .block .nav {
  display: flex;
  justify-content: space-around;
}
.container .block .nav .item {
  text-align: center;
  padding: 20rpx 0;
  position: relative;
}
.container .block .nav .item .txt {
  font-size: 32rpx;
  font-weight: 600;
  position: relative;
  z-index: 2;
}
.container .block .nav .item .active {
  position: absolute;
  content: '';
  width: 100%;
  padding: 0 24rpx;
  height: 24rpx;
  left: 50%;
  bottom: 12rpx;
  transform: translateX(-50%);
  background: #FFDE4E;
  z-index: 1;
}
.container .block .list {
  padding: 10rpx 14rpx 20rpx 28rpx;
}
.container .block .list .item {
  padding: 10rpx 0;
  display: flex;
  align-items: center;
}
.container .block .list .item .avatar {
  width: 42rpx;
  height: 42rpx;
  border-radius: 100%;
}
.container .block .list .item .name {
  padding-left: 12rpx;
  font-size: 24rpx;
  flex: 1;
  box-sizing: border-box;
}
.container .block .list .item .name .tips {
  padding-left: 8rpx;
}
.container .block .list .item .price {
  font-size: 24rpx;
}
.container .block .help {
  display: flex;
  align-items: center;
  padding-bottom: 48rpx;
}
.container .block .help .line {
  border-top: 2rpx solid #CCCCCC;
  flex: 1;
}
.container .block .help .txt {
  padding: 0 24rpx;
  font-size: 24rpx;
  color:#999999;
}


.container .canvas-wrapper {
  background: rgba(0, 0, 0, .4);
  position: fixed;
  top: 0;
  right: 0;
  left:0;
  bottom: 0;
  z-index: 99;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .startcut {
  background: rgba(0, 0, 0, 0.4);
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10000;
}
.container .startcut .main {
  width: 478rpx;
  height: 304rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  border-radius: 10rpx;
  background: #fff;
  position: relative;
  padding-top: 0;
}

.container .startcut .main .icon {
  width: 110rpx;
  height: 110rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: -78rpx;
}
.container .startcut .main .close {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  bottom: -82rpx;
  left: 50%;
  transform: translateX(-50%);
  padding: 10rpx;
}
.container .startcut .main .content {
  font-size: 28rpx;
}
.container .startcut .main .txt {
  font-size: 36rpx;
  font-weight: bold;
}
.container .startcut .main .content .price {
  color: #FF6600;
}
.container .startcut .main .btn {
  width: 347rpx;
  height: 68rpx;
  text-align: center;
  line-height: 68rpx;
  border-radius: 34rpx;
  font-size: 28rpx;
  background: #FFDE4E;
  margin-top: 48rpx;
}