.collect_title {
  width: 100%;
  padding: 20rpx 24rpx;
  color: #666;
  font-size: 24rpx;
  background: #f8f8f8;
}

.collect_box {
  padding: 0 24rpx;
}

.collect_view {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
}

.collect_viewImg {
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
  margin-right: 32rpx;
}

.collect_right {
  width: 22rpx;
  height: 22rpx;
}

.collect_viewText {
  color: #666;
  font-size: 24rpx;
  flex: 1;
}

.collect_viewText_name {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 16rpx;
}

.collect_viewText_nameText {
  color: #333;
  font-size: 28rpx;
}

.notlist {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.notlist .icon {
  width: 350rpx;
  height: 350rpx;
}

.notlist .txt {
  padding-top: 60rpx;
  font-size: 24rpx;
}
