.container .tips {
  position: fixed;
  top: 30rpx;
  right: 90rpx;
}

.container .tips .txt {
  background: #ffde4e;
  border-radius: 22rpx;
  font-size: 20rpx;
  width: 340rpx;
  height: 43rpx;
  line-height: 43rpx;
