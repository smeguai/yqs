.container .banner {
    background: #fff;
}
.container .banner .list {
    padding: 0 24rpx;
    height: 200rpx;
}
.container .banner .list .item .banner-img {
    width: 100%;
}
.container .banner .nums {
    padding-top: 14rpx;
    display: flex;
    justify-content: center;
}
.container .banner .nums .item {
    background: #E9E9E9;
    width: 8rpx;
    height: 8rpx;
    border-radius: 4rpx;
    margin: 0 13rpx;
}
.container .banner .nums .active {
    background: #FDE223;
    width: 20rpx;
    height: 8rpx;
    border-radius: 4rpx;
}
.container .nav {
    padding: 24rpx 0 46rpx 0;
    background: #fff;
}
.container .nav .list {
    display: flex;
    align-items: center;
    justify-content: center;
}
.container .nav .list:nth-child(2) {
    padding-top: 45rpx;
}
.container .nav .list .item {
    flex: 1;
    font-size: 0;
    text-align: center;
}
.container .nav .list .item .icon {
    width: 82rpx;
    height: 82rpx;
}
.container .nav .list .item .img {
    width: 44rpx;
    height: 44rpx;
}
.container .nav .list .item .txt {
    font-size: 24rpx;
    padding-top: 20rpx;
}
.container .favorable {
    padding: 36rpx 24rpx 48rpx 24rpx;
    box-sizing: border-box;
    background: #fff;
    margin-top: 20rpx;
}
.container .favorable .top {
    display: flex;
}
.container .favorable .top .txt {
    font-weight: 500;
    font-size: 32rpx;
}
.container .favorable .top .more {
    color: #FF7527;
    font-size: 20rpx;
    flex: 1;
    text-align: right;
}
.container .favorable .scroll  {
    padding-top: 28rpx;
}
.container .favorable .scroll .list {
    display: flex;
}
.container .favorable .scroll .list .item {
    width: 240rpx;
    padding-left: 30rpx;
}
.container .favorable .scroll .list .item:nth-child(1) {
    padding: 0;
}
.container .favorable .scroll .list .item .img {
    width: 240rpx;
    height: 188rpx;
    border-radius: 8rpx;
}
.container .favorable .scroll .list .item .title {
    font-size: 28rpx;
}
.container .favorable .scroll .list .item .subtitle {
    font-size: 20rpx;
    color: #999;
}
.container .favorable .scroll .list .item .newprice {
    font-size: 32rpx;
    color: #FF6600;

}
.container .favorable .scroll .list .item .newprice .f {
    font-size: 20rpx;
}
.container .favorable .scroll .list .item .oldprice {
    font-size: 20rpx;
    color: #999999;
    position: relative;
    text-decoration:line-through;
}
.container .recommend {
    margin-top: 20rpx;
    background: #fff;
    padding: 0 24rpx;
}
.container .recommend .title {
    font-size: 28rpx;
    padding-top: 30rpx;
}
.container .recommend .item {
    display: flex;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #F6F6F6;
}
.container .recommend .item:last-child {
    border: 0;
}
.container .recommend .item .img {
    width: 128rpx;
    height: 128rpx;
    border-radius: 10rpx;
}
.container .recommend .item .desc {
    flex: 1;
    padding: 0 28rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
}
.container .recommend .item .desc .top {
    font-size: 0;
}
.container .recommend .item .desc .sub {
    font-size: 16rpx;
    color: #666666;
    padding: 12rpx 0;
}
.container .recommend .item .desc .sub .txt0 {
    font-size: 20rpx;
    padding-right: 6rpx;
    vertical-align: middle;
}
.container .recommend .item .desc .sub .txt1 {
    font-size: 20rpx;
    padding-left: 6rpx;
    vertical-align: middle;
}
.container .recommend .item .desc .seller {
    font-size: 28rpx;
    font-weight: 600;
}
.container .recommend .item .desc .tab {
    display: flex;
}
.container .recommend .item .desc .tabli {
    font-size: 20rpx;
    border: 1rpx solid #999;
    padding: 8rpx 10rpx;
    border-radius: 6rpx;
    margin-left: 16rpx;
}
.container .recommend .item .desc .tabli:nth-child(1) {
    margin-left: 0;
}
.container .recommend .item .addr {
    font-size: 20rpx;
    padding-top: 40rpx;
}