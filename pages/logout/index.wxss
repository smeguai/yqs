.page {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 0 24rpx;
}
.logout_icon {
    padding-top: 78rpx;
}
.logout_name {
    color: #333333;
    font-size: 32rpx;
    padding-top: 39rpx;
}
.logout_hint {
    color: #666666;
    font-size: 24rpx;
    padding-top: 27rpx;
}
.logout_view {
    width: 100%;
    height: 300rpx;
    background: #FFFFFF;
    border-radius:10rpx;
    margin-top: 68rpx;
}
.logout_viewTop {
    margin: 0 24rpx;
    padding: 36rpx 0 38rpx;
    border-bottom: 1rpx solid #E0E0E0;
}
.logout_viewTop_text {
    color: #333333;
    font-size: 24rpx;
    display: flex;
    align-items: center;
}
.logout_viewTopDot {
    width: 6rpx;
    height: 6rpx;
    border-radius: 50%;
    background-color: #333333;
    margin-right: 22rpx;
}
.logout_viewText {
    padding: 40rpx 0;
    margin: 0 24rpx;
    color: #666666;
    font-size: 24rpx;
    line-height: 40rpx;
}

.logout_clause {
    width: 100%;
    display: flex;
    align-items: center;
    margin: 50rpx 0;
}
.logout_clause>image {
    width: 24rpx;
    height: 24rpx;
    border: 1rpx solid #999999;
    border-radius: 50%;
    margin-right: 12rpx;
}
.logout_clause_text {
    color: #333333;
    font-size: 24rpx;
}
.logout_btn {
    width: 100%;
    height: 74rpx;
    line-height: 74rpx;
    text-align: center;
    color: #333333;
    font-size: 28rpx;
    border-radius: 50rpx;
}
.logout_confirm {
    background: #CCCCCC;
    border: none;
    outline: none;
}
.logout_cancel {
    border:1px solid #999999;
    background: #FFFFFF;
    margin-top: 50rpx;
}