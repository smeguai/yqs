.container {
  background: #fff;
}
.container .header {
  padding: 24rpx 30rpx;
  background: #F9F9F9;
  font-size: 24rpx;
  color: #666666;
}
.container .item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 34rpx;
  border-bottom: 1rpx solid #F5F5F5;
}
.container .item text {
  flex: 1;
}
.container .item .icon {
  width: 28rpx;
  height: 28rpx;
}