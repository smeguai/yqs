.container .header {
  display: flex;
  justify-content: space-between;
  background: #fff;
  border-bottom: 1rpx solid #f0f0f0;
  padding: 50rpx 24rpx 30rpx 24rpx;
}

.container .header .avatar {
  width: 150rpx;
  height: 150rpx;
  border-radius: 6rpx;
}

.container .header .info {
  padding-left: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex: 1;
  width: 0;
  box-sizing: border-box;
}

.container .header .info .strong {
  font-size: 32rpx;
  font-weight: bold;
  flex: 1;
  width: 0;
  padding-right: 24rpx;
}

.container .header .info .score {
  display: flex;
  align-items: center;
}
.container .header .info .top {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}
.container .header .info .top .score_rt {
  width: 120rpx;
  text-align: center;
  font-size: 0;
}
.container .header .info .top .score_rt .collect {
  width: 30rpx;
  height: 28rpx;
}
.container .header .info .top .score_rt view {
  font-size: 20rpx;
  padding-top: 4rpx;
}
.container .header .info .score .count {
  font-size: 20rpx;
  color: #ff634c;
  padding-left: 10rpx;
}

.container .header .info .midd {
  display: flex;
  align-items: center;
}

.container .header .info .midd .lf {
  display: flex;
  align-items: center;
  border-right: 1rpx solid #999;
  padding-right: 32rpx;
  flex: 1;
  width: 0;
}

.container .header .info .midd .lf .icon {
  width: 18rpx;
  height: 24rpx;
}

.container .header .info .midd .lf .txt {
  font-size: 24rpx;
  padding-left: 4rpx;
  display: flex;
}
.container .header .info .midd .lf .txt .dis {
  padding-right: 8rpx;
  padding-left: 4rpx;
}
.container .header .info .midd .tel {
  width: 28rpx;
  height: 28rpx;
  padding: 10rpx 20rpx;
}

.container .header .info .tags {
  display: flex;
  flex-wrap: wrap;
}

.container .header .info .tags .item {
  padding: 6rpx 12rpx;
  border: 1rpx solid #999;
  border-radius: 4rpx;
  margin: 0 16rpx 12rpx 0;
  font-size: 20rpx;
}

.container .welfare {
  padding: 20rpx 24rpx;
  background: #fff;
  margin-bottom: 20rpx;
}

.container .welfare .btn {
  margin-top: 20rpx;
  background: #ffde4e;
  font-size: 32rpx;
  font-weight: bold;
}

.container .welfare .item-wrapper {
  padding: 18rpx 0;
}

.container .welfare .item-wrapper .scroll .list {
  white-space: nowrap;
}

.container .welfare .item-wrapper .item0 {
  margin-right: 28rpx;
  display: inline-block;
  box-sizing: border-box;
  width: 504rpx;
}

.container .welfare .item-wrapper .item1 {
  flex: 1;
}

.container .welfare .item-wrapper .item {
  background: #fffaef;
  border: 1rpx solid #ffde4e;
  border-radius: 10rpx;
  padding: 6rpx 20rpx;
  align-items: center;
  position: relative;
  box-sizing: border-box;
}

.container .welfare .item-wrapper .item::after {
  content: '';
  position: absolute;
  left: -10rpx;
  top: 50%;
  box-sizing: border-box;
  transform: translateY(-50%);
  width: 20rpx;
  height: 20rpx;
  border-radius: 100%;
  background: #fff;
  border-right: 1rpx solid #ffde4e;
}

.container .welfare .item-wrapper .item::before {
  content: '';
  position: absolute;
  right: -10rpx;
  top: 50%;
  box-sizing: border-box;
  transform: translateY(-50%);
  width: 20rpx;
  height: 20rpx;
  border-radius: 100%;
  background: #fff;
  border-left: 1rpx solid #ffde4e;
}

.container .welfare .item-wrapper .item .txt {
  font-size: 26rpx;
  font-weight: bold;
}

.container .welfare .item-wrapper .item .info {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.container .welfare .item-wrapper .item .info .activedate {
  font-size: 22rpx;
  color: #666;
  padding-right: 10rpx;
}

.container .welfare .item-wrapper .item .price {
  font-size: 46rpx;
  font-weight: bold;
  color: #f60;
  display: flex;
  align-items: flex-end;
}

.container .welfare .item-wrapper .item .price .tip {
  padding-left: 12rpx;
  font-size: 24rpx;
  font-weight: 400;
  vertical-align: bottom;
}

.container .welfare .item-wrapper .item .price .tag {
  font-size: 20rpx;
  color: #f60;
  border-radius: 10rpx;
  padding: 4rpx 6rpx;
  border: 1rpx solid #f60;
  font-weight: 400;
  margin-left: 16rpx;
}

.container .welfare .item-wrapper .item .price .oldprice {
  font-size: 24rpx;
  color: #999;
  padding-left: 16rpx;
  font-weight: 400;
  vertical-align: bottom;
}

.container .welfare .item-wrapper .item .info  .btn {
  width: 120rpx;
  height: 52rpx;
  border-radius: 26rpx;
  font-size: 26rpx;
  font-weight: bold;
  text-align: center;
  line-height: 52rpx;
  padding: 0;
}

.container .welfare .item-wrapper .item .info .b0 {
  background: #ffde4e;
  margin-top: 0;
}

.container .welfare .item-wrapper .item .info .b1 {
  color: #999;
  font-weight: normal;
  background: transparent;
  border: 1rpx solid #ffde4e;
  margin-top: 0;
}

.container .welfare .item-wrapper .item .info .b2 {
  color: #fff;
  background: #f60;
  margin-top: 0;
}

.container .discount {
