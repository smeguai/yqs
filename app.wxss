.container {
  min-height: 100vh;
  color: #333;
  background: #F8F8F8;
} 
.str {
    white-space:nowrap;
    word-break:keep-all;
    overflow:hidden;
    text-overflow:ellipsis;
}
.strmore {
    display:-webkit-box;
    overflow:hidden;
    text-overflow:ellipsis;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
}
.del {
    text-decoration:line-through;
}


.container .search {
    background: #f4f4f4;
    border-radius: 24rpx;
    display: flex;
    align-items: center;
    padding: 4rpx 30rpx 0 30rpx;
    box-sizing: border-box;
    margin: 28rpx 0;
    height: 60rpx;
}

.container .search .icon {
    width: 24rpx;
    height: 24rpx;
}

.container .search .ipt {
    flex: 1;
    font-size: 26rpx;
    padding-left: 20rpx;
}