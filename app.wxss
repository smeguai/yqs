.container {
  min-height: 100vh;
  color: #333;
  background: #F8F8F8;
} 
.str {
    white-space:nowrap;
    word-break:keep-all;
    overflow:hidden;
    text-overflow:ellipsis;
}
.strmore {
    display:-webkit-box;
    overflow:hidden;
    text-overflow:ellipsis;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
}
.del {
    text-decoration:line-through;
}