.info_stepInputImgBox {
    padding: 40rpx 0;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}
.info_stepInputImg {
    width:148rpx;
    height:148rpx;
    background:rgba(249,249,249,1);
    border:1rpx solid rgba(233,233,233,1);
    border-radius:10rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin: 10rpx 20rpx;
}
.info_stepInputImg>image {
    width: 60rpx;
    height: 60rpx;
    margin-bottom: 10rpx;
}
.info_stepInputImg>view {
    color: #CCCCCC;
    font-size: 20rpx;
}
.info_stepInputImgBox_view {
    position: relative;
    width:148rpx;
    height:148rpx;
    border-radius:10rpx;
    margin: 10rpx 20rpx;
}
.info_stepInputImgBox_viewImg {
    width:148rpx;
    height:148rpx;
    border-radius:10rpx;
}
.info_stepInputImgBox_viewIcon {
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    top: 0;
    right: 0;
    margin-right: -10rpx;
    margin-top: -10rpx;
}